{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
     
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "types":["next-auth"],
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts","types/**/*.d.ts", "src/**/*.ts", "src/**/*.tsx", ".next/types/**/*.ts","types/**/*.d.ts", "types.ts"],
  "exclude": ["node_modules"]
}
